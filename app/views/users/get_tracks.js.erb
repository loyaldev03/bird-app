var tracks = [];
<% @tracks[0..10].each do |track| %>

  tracks.push({ 
    'release_id': '<%= track.release.id %>',
    'track_number': '<%= track.track_number %>',
    'id': '<%= track.id %>',
    'title': '<%= track.title %>', 
    'artists': '<%= track.artists %>', 
    'mp3': '<%= track.stream_uri %>'
  });
<% end %>

if (tracks.length > 0) {
  addToPlaylist(tracks, true);
  // setPlaylist(tracks, true);


}