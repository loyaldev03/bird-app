var tracks = <%= raw @tracks.to_json %>;

setPlaylist(tracks, true, 0, 0, true);

switch('<%= @source %>') {
  case 'playlist':
    // code block
    break;
  case 'track':
    $("[class^='playlist-play-track-']").removeClass('playing')
        .closest('tr').removeClass('active');
    $('.playlist-play-track-' + tracks[0].id).addClass('playing')
        .closest('tr').addClass('active');
    break;
  case 'user':
    // code block
    break;
  case 'release':
    // code block
    break;
}